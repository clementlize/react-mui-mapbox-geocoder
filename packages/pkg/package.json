{
  "name": "react-mui-mapbox-geocoder",
  "version": "1.3.3",
  "description": "A Material UI Autosuggest'ing Mapbox Geocoder for locating addresses and points of interest.",
  "repository": {
    "type": "git",
    "url": "https://github.com/wheredoesyourmindgo/react-mui-mapbox-geocoder"
  },
  "keywords": [
    "geocoding",
    "geocoder",
    "address",
    "mapbox",
    "material-ui",
    "react",
    "autosuggest"
  ],
  "main": "dist/index.js",
  "license": "MIT",
  "source": "src/index.ts",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build",
